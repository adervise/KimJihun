<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<!-- biz_log -->
<sqlMap namespace="bigLog">

    <insert id="BizlogRecordDAO.insertBizLog">
        INSERT INTO TB_BD_BIZ_LOG (
			 CTGRY_LCLAS
			,CTGRY_MLSFC
			,CTGRY_SCLAS
			,LOG_INFO
			,BIZ_SQL		)
		VALUES (
		      #appId#
		    , #programeId#
		    , #viewId#
			, #logInfo#
			, #bizSql#
        )
    </insert>
</sqlMap>
